<?xml version="1.0" encoding="utf-8" ?>
<ContentPage x:Class="MauiMemoryLeakTestApp.SecondaryPage"
             xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml">
    <VerticalStackLayout>
        <Label HorizontalOptions="Center"
               Text="Welcome to .NET MAUI!"
               VerticalOptions="Center" />
        <Button x:Name="OpenTertiaryPageButton"
                Clicked="OpenTertiaryPageButton_Clicked"
                Text="Open tertiary page" WidthRequest="200" />
        <Button x:Name="GCCollectButton"
                Clicked="GCCollectButton_Clicked"
                Text="GC Collect"
                WidthRequest="200"/>
    </VerticalStackLayout>
</ContentPage>
